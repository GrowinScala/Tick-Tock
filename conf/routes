# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /                 controllers.TaskController.index
GET     /scheduledTasks   controllers.TaskController.getSchedule
GET     /schedule/:id     controllers.TaskController.getScheduleById(id: Int)
GET     /getFiles         controllers.FileController.getAllFiles

POST    /schedule         controllers.TaskController.schedule

#PATCH   /schedule/:id
#DELETE  /schedule/:id
#PUT     /schedule/:id